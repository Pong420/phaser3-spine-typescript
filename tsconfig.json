{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "allowUnreachableCode": false,
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "allowJs": true,
    "lib": ["dom", "es5", "esnext"],
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "pwa", "webpack"]
}
